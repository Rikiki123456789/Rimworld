  <ThingDef ParentName="AnimalThingBase">
    <defName>Sduiggles</defName>
    <label>sduiggles</label>
    <description>Found in the swamps and shallow waters. Behind the appearances, this kind of squid is very affectuous and intelligent. Who would like to fish it? You monster!</description>
    <statBases>
      <MoveSpeed>0.01</MoveSpeed>
      <ComfyTemperatureMin>-5</ComfyTemperatureMin>
      <MarketValue>100</MarketValue>
      <Mass>3</Mass>
      <LeatherAmount>0</LeatherAmount>
    </statBases>
    <verbs>
      <li>
        <verbClass>Verb_MeleeAttack</verbClass>
        <defaultCooldownTime>2.5</defaultCooldownTime>
        <meleeDamageBaseAmount>8</meleeDamageBaseAmount>
        <meleeDamageDef>ToxicBite</meleeDamageDef>
        <linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>12</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </verbs>
    <race>
      <body>Squid</body>
      <intelligence>ToolUser</intelligence>
      <predator>true</predator>
      <maxPreyBodySize>0.35</maxPreyBodySize>
      <baseBodySize>0.20</baseBodySize>
      <baseHealthScale>0.30</baseHealthScale>
      <baseHungerRate>0.07</baseHungerRate>
      <foodType>CarnivoreAnimal</foodType>
      <!--<leatherColor>(74,140,140)</leatherColor> Kept for legacy.-->
      <meatLabel>sduiggles tentacles</meatLabel>
      <wildness>0.40</wildness>
      <manhunterOnTameFailChance>0.005</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.25</manhunterOnDamageChance>
      <gestationPeriodDays>22.0</gestationPeriodDays>
      <nameOnTameChance>1</nameOnTameChance>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(3, 1)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeExpectancy>4</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.25</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.8</minAge>
          <soundWounded>Pawn_Cobra_Wounded</soundWounded>
          <soundDeath>Pawn_Cobra_Death</soundDeath>
          <soundCall>Pawn_Cobra_Call</soundCall>
          <soundAngry>Pawn_Cobra_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
    </tradeTags>
